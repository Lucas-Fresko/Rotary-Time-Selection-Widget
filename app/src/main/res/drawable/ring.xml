<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="40dp"
        android:right="40dp"
        android:bottom="40dp"
        android:left="40dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/transparent" />
        </shape>
    </item>
    <item
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp"
        android:left="5dp">
        <shape
            android:shape="oval">
            <stroke android:width="50dp"
                android:color="@color/teal_500"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <stroke android:width="5dp"
                android:color="@color/teal_900"/>
        </shape>
    </item>
    <item
        android:top="55dp"
        android:right="55dp"
        android:bottom="55dp"
        android:left="55dp">
        <shape
            android:shape="oval">
            <stroke android:width="5dp"
                android:color="@color/teal_900"/>
        </shape>
    </item>
</layer-list>
